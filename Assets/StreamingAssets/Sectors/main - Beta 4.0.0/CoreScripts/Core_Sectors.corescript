Function(
    name=Legend_triggerCutscene,
    sequence=(
        AddIntValues(val1=$$$Legend_pylonsMerged, val2=1, name=$$$Legend_pylonsMerged),
        ConditionBlock(
            Comparison(
                val1=$$$Legend_pylonsMerged,
                val2=1,
                comp=Eq,
                sequence=(
                    Call(Legend_cs2)
                )
            ),
            Comparison(
                val1=$$$Legend_pylonsMerged,
                val2=2,
                comp=Eq,
                sequence=(
                    Call(Legend_cs3)
                )
            ),
            Comparison(
                val1=$$$Legend_pylonsMerged,
                val2=3,
                comp=Eq,
                sequence=(
                    Call(Legend_cs4)
                )
            ),
        )
    )
)


